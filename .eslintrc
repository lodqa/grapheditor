{
  "rules": {
    "indent-legacy": [1, 2],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      1,
      "never"
    ],
    "no-console": 0,
    "no-label-var": 2,
    "comma-dangle": 2,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 2,
    "no-empty": 1,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "prefer-reflect": 2,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 2,
    "no-unreachable": 2,
    "use-isnan": 2,
    "valid-jsdoc": 2,
    "valid-typeof": 2,
    "no-unexpected-multiline": 2,
    "accessor-pairs": ["error", { "setWithoutGet": false }],
    "block-scoped-var": 2,
    "complexity": 2,
    "consistent-return": 2,
    "prefer-spread": 2,
    "default-case": 2,
    "dot-notation": 2,
    "no-this-before-super": 2,
    "no-dupe-class-members": 2,
    "guard-for-in": 2,
    "no-alert": 2,
    "no-caller": 2,
    "no-div-regex": 2,
    "no-const-assign": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-implicit-coercion": 2,
    "no-implied-eval": 2,
    "no-invalid-this": 2,
    "no-iterator": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-new": 2,
    "no-octal-escape": 2,
    "no-octal": 2,
    "no-class-assign": 2,
    "no-process-env": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-throw-literal": 2,
    "generator-star-spacing": 2,
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "no-void": 2,
    "no-warning-comments": 2,
    "no-with": 2,
    "constructor-super": 2,
    "arrow-spacing": 2,
    "wrap-iife": 2,
    "yoda": 2,
    "strict": 2,
    "wrap-regex": 2,
    "spaced-comment": 2,
    "no-delete-var": 2,
    "no-cond-assign": 2,
    "no-shadow-restricted-names": 2,
    "space-unary-ops": 2,
    "no-undef-init": 2,
    "no-undef": 2,
    "no-unused-vars": 2,
    "space-infix-ops": 2,
    "callback-return": 2,
    "global-require": 2,
    "handle-callback-err": 2,
    "no-mixed-requires": 2,
    "no-new-require": 2,
    "no-path-concat": 2,
    "no-process-exit": 2,
    "no-restricted-modules": 2,
    "no-sync": 2,
    "array-bracket-spacing": 2,
    "block-spacing": 2,
    "brace-style": 1,
    "camelcase": 2,
    "comma-spacing": 2,
    "comma-style": 2,
    "computed-property-spacing": 2,
    "consistent-this": 2,
    "eol-last": 2,
    "space-in-parens": 2,
    "space-before-blocks": 2,
    "id-match": 2,
    "jsx-quotes": 2,
    "key-spacing": 2,
    "keyword-spacing": 1,
    "lines-around-comment": 2,
    "max-nested-callbacks": 2,
    "new-cap": 2,
    "semi-spacing": 2,
    "no-array-constructor": 2,
    "no-continue": 2,
    "operator-linebreak": 2,
    "no-lonely-if": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": 2,
    "no-nested-ternary": 2,
    "no-new-object": 2,
    "no-restricted-syntax": 2,
    "no-spaced-func": 2,
    "operator-assignment": 2,
    "no-trailing-spaces": 1,
    "object-curly-spacing": 2,
    "no-unneeded-ternary": 2,
    "require-yield": 2,
    "new-parens": 2,
    "prefer-arrow-callback": 2,
    "eqeqeq": 2,
    "no-var": 2,
    "no-unused-expressions": 2,
    "no-undefined": 2,
    "vars-on-top": 2,
    "no-catch-shadow": 2,
    "no-inline-comments": 0,
    "no-extra-parens": [1, "all", {"returnAssign": false}],
    "prefer-template": 2,
    "no-ternary": 0,
    "no-param-reassign": 2,
    "no-else-return": 2,
    "func-names": ["error", "never"],
    "one-var": [1, "never"],
    "padded-blocks": [2, "never"],
    "quote-props": [2, "as-needed"],
    "space-before-function-paren": [2, "never"],
    "func-style": 0,
    "arrow-parens": 2,
    "no-underscore-dangle": 0,
    "radix": 0,
    "dot-location": 0,
    "require-jsdoc": 0,
    "no-use-before-define": [2, "nofunc"],
    "id-length": 0,
    "newline-after-var": 2,
    "sort-vars": 0,
    "curly": 2,
    "object-shorthand": 1,
    "prefer-const": 1
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  }
}
